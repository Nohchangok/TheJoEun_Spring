<?xml version="1.0" encoding="UTF-8"?>

<!-- 쿼리문을 맵핑시켜주는 dtd -->
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="springMybatis0204.mapper.DepartmentMapper">
	<insert id="insertDepartment" keyProperty="id"
		parameterType="dept">
		insert into department values(#{id}, #{deptName},
		#{deptLoc})
	</insert>
</mapper>